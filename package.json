{
  "name": "ng2-daum-address",
  "version": "1.1.5",
  "description": "Daum Address API for Angular2",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf bundle && tsc",
    "prepublishOnly": "npm run build",
    "lint": "eslint lib/**/*.{ts,tsx} --quiet --fix"
  },
  "author": "Gracefullight",
  "license": "MIT",
  "typings": "./index.d.ts",
  "keywords": [
    "angular2",
    "ng2",
    "Daum",
    "DaumAddress",
    "daumapi"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gracefullight/ng2-daum-address.git"
  },
  "bugs": {
    "url": "http://github.com/gracefullight/ng2-daum-address/issues",
    "email": "gracefullight.dev@gmail.com"
  },
  "homepage": "https://ng2-daum-address.firebaseapp.com/",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "lib/**/*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "peerDependencies": {
    "@angular/core": "^4.4.7",
    "@angular/compiler": "^4.4.7",
    "@angular/common": "^4.4.7"
  },
  "devDependencies": {
    "@angular/common": "^4.4.7",
    "@angular/compiler": "^4.4.7",
    "@angular/core": "^4.4.7",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "es6-promise": "^4.1.0",
    "es6-shim": "^0.35.3",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.12",
    "typescript": "^2.9.2",
    "zone.js": "^0.8.29"
  }
}
